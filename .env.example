# Polygon.io API Key
VITE_POLYGON_API_KEY=your_polygon_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_SUPABASE_EDGE_FUNCTION_URL=https://your-project-id.supabase.co/functions/v1/polygon-data 

# Feature Flags
VITE_ENABLE_REALTIME_UPDATES=false
VITE_ENABLE_POLYGON_WEBSOCKET=false
VITE_USE_MOCK_DATA=false

# Cache Settings
VITE_DATA_CACHE_TTL_SECONDS=60
VITE_MAX_CACHE_ENTRIES=1000 